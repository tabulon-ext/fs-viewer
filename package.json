{
  "name": "fs-viewer",
  "version": "0.1.0",
  "description": "A tagging small media browser",
  "main": "build/index.js",
  "scripts": {
    "test-types": "tsc",
    "build": "webpack --mode=production",
    "watch": "webpack --mode=development --watch",
    "postinstall": "electron-builder install-app-deps",
    "release": "npm run clean && npm run build && electron-builder",
    "clean": "rm -rf build dist",
    "start": "electron .",
    "test": "jest",
    "test-watch": "jest --watch"
  },
  "build": {
    "appId": "de.unreadableco.freakshow.viewer",
    "copyright": "Copyright Â© 2020 ${author}",
    "linux": {
      "target": [
        "deb",
        "rpm"
      ],
      "category": "Graphics"
    },
    "files": [
      "build/*",
      "!build/*.txt",
      "!node_modules/**/*"
    ],
    "extraFiles": [
      {
        "from": "build",
        "filter": [
          "*.txt"
        ]
      }
    ],
    "publish": null
  },
  "repository": "https://github.com/unreadablewxy/fs-viewer",
  "keywords": [],
  "author": {
    "name": "UnreadableCode",
    "email": "nobody@unreadableco.de",
    "url": "https://github.com/unreadable-code"
  },
  "homepage": "https://github.com/unreadablewxy/fs-viewer",
  "license": "GPL-2.0",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^25.2.1",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@types/react-router-dom": "^5.1.5",
    "@types/reselect": "^2.2.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.3",
    "electron": "^8.2.5",
    "electron-builder": "^22.6.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^25.5.4",
    "license-webpack-plugin": "^2.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "native-ext-loader": "^2.3.0",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^2.3.6",
    "ts-loader": "^7.0.3",
    "tslint": "^6.1.2",
    "tslint-immutable": "^6.0.1",
    "tslint-webpack-plugin": "^2.1.0",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-config-builder": "git+https://github.com/unreadable-code/webpack-config-builder.git"
  },
  "optionalDependencies": {
    "fs-xattr": "^0.3.1",
    "shell-image-win": "github:unreadable-code/shell-image-win#upgrade-nan"
  },
  "dependencies": {
    "@mdi/js": "^5.1.45",
    "@mdi/react": "^1.4.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "reselect": "^4.0.0"
  }
}
